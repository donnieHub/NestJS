{
  "name": "r-omni",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/r-omni/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/r-omni/test/jest-e2e.json",
    "migration:users:create": "node -r dotenv/config ./node_modules/.bin/mikro-orm migration:create --config=apps/user/mikro-orm.config.ts",
    "migration:users:up": "node -r dotenv/config ./node_modules/.bin/mikro-orm migration:up --config=apps/user/mikro-orm.config.ts",
    "migration:users:down": "node -r dotenv/config ./node_modules/.bin/mikro-orm migration:down --config=apps/user/mikro-orm.config.ts",
    "migration:booking:create": "node -r dotenv/config ./node_modules/.bin/mikro-orm migration:create --config=apps/booking/mikro-orm.config.ts",
    "migration:booking:up": "node -r dotenv/config ./node_modules/.bin/mikro-orm migration:up --config=apps/booking/mikro-orm.config.ts",
    "migration:booking:down": "node -r dotenv/config ./node_modules/.bin/mikro-orm migration:down --config=apps/booking/mikro-orm.config.ts",
    "migration:room:create": "node -r dotenv/config ./node_modules/.bin/mikro-orm migration:create --config=apps/rooms/mikro-orm.config.ts",
    "migration:room:up": "node -r dotenv/config ./node_modules/.bin/mikro-orm migration:up --config=apps/rooms/mikro-orm.config.ts",
    "migration:room:down": "node -r dotenv/config ./node_modules/.bin/mikro-orm migration:down --config=apps/rooms/mikro-orm.config.ts",
    "seed:room:buildings": "npx mikro-orm seeder:run --class BuildingSeeder --config=apps/rooms/mikro-orm.config.ts",
    "seed:room:room": "npx mikro-orm seeder:run --class RoomSeeder --config=apps/rooms/mikro-orm.config.ts",
    "seed:room:availability": "npx mikro-orm seeder:run --class RoomAvailabilitySeeder --config=apps/rooms/mikro-orm.config.ts",
    "seed:room:all": "npx mikro-orm seeder:run --class DatabaseSeeder --config=apps/rooms/mikro-orm.config.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@grpc/grpc-js": "1.12.6",
    "@mikro-orm/core": "^6.4.16",
    "@mikro-orm/migrations": "^6.4.16",
    "@mikro-orm/nestjs": "^6.1.1",
    "@mikro-orm/postgresql": "^6.4.16",
    "@mikro-orm/seeder": "^6.4.16",
    "@nestjs-plugins/nestjs-nats-jetstream-transport": "2.2.6",
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/common": "11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "11.0.1",
    "@nestjs/cqrs": "11.0.1",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/jwt": "11.0.1",
    "@nestjs/microservices": "^11.1.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "11.0.1",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/serve-static": "^5.0.3",
    "@rsdk/builtin-contract": "5.10.0",
    "@rsdk/common": "5.10.0",
    "@rsdk/core": "5.10.0",
    "@rsdk/decorators": "5.10.0",
    "@rsdk/grpc.clients": "5.10.0",
    "@rsdk/grpc.server": "5.10.0",
    "@rsdk/logging": "5.10.0",
    "@rsdk/metadata": "5.10.0",
    "@rsdk/nats.common": "5.10.0",
    "@rsdk/nats.producer": "5.10.0",
    "@rsdk/nats.producer.direct": "5.10.0",
    "@rsdk/nats.transport": "5.10.0",
    "@rsdk/nest-tools": "5.10.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.1",
    "graphql": "^16.11.0",
    "nats": "^2.29.3",
    "nice-grpc": "2.1.11",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "pug": "^3.0.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@mikro-orm/cli": "^6.4.16",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
